{
  "name": "acyort-paginator",
  "version": "0.1.3",
  "description": "Paginator for AcyOrt",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:cover && npm run clean",
    "test:unit": "mocha --reporter dot --require intelli-espower-loader ./test/index.js",
    "test:lint": "eslint .",
    "test:cover": "istanbul cover --print both _mocha ./test/index.js",
    "clean": "rm -R ./test/page ./test/categories ./test/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acyortjs/acyort-paginator.git"
  },
  "keywords": [
    "pagination",
    "acyort",
    "helper"
  ],
  "author": "LoeiFy@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acyortjs/acyort-paginator/issues"
  },
  "homepage": "https://github.com/acyortjs/acyort-paginator#readme",
  "devDependencies": {
    "acyort": "^2.2.1",
    "acyort-config": "^1.0.2",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "intelli-espower-loader": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "power-assert": "^1.4.4"
  }
}
